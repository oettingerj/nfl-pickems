{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "function": "svelteKit"
      }
    ],
    "predeploy": "yarn run build"
  },
  "functions": {
    "source": "functions"
  }
}
